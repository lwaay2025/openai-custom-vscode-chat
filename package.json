{
	"name": "openai-custom-vscode-chat",
	"publisher": "lijcoder",
	"displayName": "OpenAI Custom Provider for GitHub Copilot Chat",
	"description": "An extension that integrates OpenAI Custom Inference Providers into GitHub Copilot Chat",
	"icon": "assets/logo.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/lijcoder/openai-custom-vscode-chat"
	},
	"version": "0.0.7",
	"engines": {
		"vscode": "^1.104.0"
	},
	"categories": [
		"AI",
		"Chat"
	],
	"galleryBanner": {
		"color": "#100f11",
		"theme": "dark"
	},
	"badges": [
		{
			"url": "https://img.shields.io/github/stars/lijcoder/openai-custom-vscode-chat?style=social",
			"description": "Star openai-custom-vscode-chat on Github",
			"href": "https://github.com/lijcoder/openai-custom-vscode-chat"
		}
	],
	"homepage": "https://github.com/lijcoder/openai-custom-vscode-chat",
	"bugs": {
		"url": "https://github.com/lijcoder/openai-custom-vscode-chat/issues"
	},
	"license": "MIT",
	"contributes": {
		"languageModelChatProviders": [
			{
				"vendor": "openai-custom",
				"displayName": "OpenAI Custom",
				"managementCommand": "openai.custom.manage"
			}
		],
		"commands": [
			{
				"command": "openai.custom.manage",
				"title": "Manage openai custom Provider"
			}
		]
	},
	"activationEvents": [],
	"main": "./out/extension.js",
	"scripts": {
		"download-api": "dts dev",
		"postdownload-api": "dts main",
		"postinstall": "npm run download-api",
		"compile": "tsc -p ./",
		"lint": "eslint",
		"format": "prettier --write .",
		"watch": "tsc -watch -p ./",
		"test": "npm run compile && vscode-test",
		"vscode:prepublish": "npm run compile",
		"vscode:package": "npx @vscode/vsce package",
		"vscode:publish:patch": "npx @vscode/vsce publish patch",
		"vscode:publish:minor": "npx @vscode/vsce publish minor",
		"vscode:publish:major": "npx @vscode/vsce publish major"
	},
	"devDependencies": {
		"@eslint/js": "^9.13.0",
		"@stylistic/eslint-plugin": "^2.9.0",
		"@types/mocha": "^10.0.6",
		"@types/node": "^22",
		"@types/vscode": "^1.103.0",
		"@vscode/dts": "^0.4.1",
		"@vscode/test-cli": "^0.0.11",
		"@vscode/test-electron": "^2.5.2",
		"eslint": "^9.13.0",
		"prettier": "^3.1.0",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.39.0"
	}
}
